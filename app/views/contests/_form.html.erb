<%= simple_form_for(@contest) do |f| %>
<%= f.error_notification %>
<div class="container ">
<div class="control-group">
  <%= f.label :Firstname,class: 'control-label' %>
  <div class="medium-input">
    <%= f.text_field :first_name, id: :title, class: "form-control" %>
  </div>
</div>
<div class="control-group">
  <%= f.label :Lastname,class: 'control-label' %>
  <div class="controls">
    <%= f.text_field :last_name, id: :description, class: "form-control" %>
  </div>
</div>
<div class="control-group">
  <%= f.label :Email,class: 'control-label' %>
  <div class="controls">
    <%= f.text_field :email, id: :address, class: "form-control" %>
  </div>
</div>
<div class="control-group">
  <%= f.label :Slogan,class: 'control-label' %>
  <div class="controls">
    <%= f.text_field :slogan, id: :address, class: "form-control" %>
  </div>
</div>

<div class="actions"style="margin-top:10px;">
  <%= f.button :submit %>
</div>
<% end %>
</div>
